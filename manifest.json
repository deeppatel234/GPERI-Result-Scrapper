{
    "name": "GTU Result Scrapper",
    "version": "0.1",
    "manifest_version": 2,


    "description": "Scrape GTU Result",

    "browser_action": {
        "default_title": "GTU"
    },

    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["jquery-3.1.1.js","ocrad.js","underscore-min.js","content.js"]
        }
    ],

    "permissions": [
        "cookies",
        "http://*/*",
        "https://*/*",
        "tabs", 
        "storage",
        "<all_urls>",
        "activeTab"
    ]
}
